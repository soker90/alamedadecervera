{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    // Preact specific settings
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
		"baseUrl": ".",
		"paths": {
			"@assets/*": ["src/assets/*"],
			"@components/*": ["src/components/*"],
			"@layouts/*": ["src/layouts/*"]
		},
		"strictNullChecks": true
	}
}
